#include<bits/stdc++.h>
using namespace std;
int main(){
	int a,b,c;
	cout<<"enter the value of a:-";
	cin>>a;
	cout<<"enter the value of b:-";
	cin>>b;
	cout<<"enter the value of c:-";
	cin>>c;
	if (a>b&&a>c)
	cout<<"greatest of three numbers is "<<a;
	else if(b>c&&b>a)
    cout<<"greatest of three numbers is "<<b;
	else if(c>a&&c>b)
	cout<<"greatest of three numbers is "<<c;
}
